<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="org.gnome.shell.extensions.twitchoverlay" path="/org/gnome/shell/extensions/twitchoverlay/">
    <key name="x-position" type="d">
      <default>1</default>
    </key>
    <key name="y-position" type="d">
      <default>0.5</default>
    </key>
    <key name="twitch-channel" type="s">
      <default>"ghostly_vagabond"</default>
    </key>
    <key name="window-regex" type="s">
      <default>"Path of Exile"</default>
    </key>
    <key name="scrollback" type="d">
      <default>25</default>
    </key>
    <key name="disable-unredirect" type="b">
      <default>false</default>
    </key>
    <key name="chat-background-color" type="s">
      <default>"rgba(0, 0, 0, 0.5)"</default>
    </key>
    <key name="chat-text-color" type="s">
      <default>"rgba(255, 255, 255, 1)"</default>
    </key>
    <key name="chat-width" type="d">
      <default>512</default>
    </key>
    <key name="chat-font" type="s">
      <default>"Sans 6"</default>
    </key>
  </schema>
</schemalist>